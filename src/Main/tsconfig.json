{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "../../build/Main",
    "composite": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "files": ["./preload.ts","./main.ts" ],
}